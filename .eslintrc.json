{
	"env": {
		"browser": true,
		"es2021": true
	},
	"plugins": [
		"react"
	],
	"extends": [
		"plugin:react/recommended",
		"eslint:recommended"
	],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	
	"rules": {
		"indent": ["error", "tab"]
	}
}

